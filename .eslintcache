gn: 'left',\n        paddingLeft: 80,\n        fontColor: 'black',\n    },\n    button1:{\n        backgroundColor: 'pink',\n        color: 'black',\n        width: '35%',\n        borderRadius:40,\n        marginTop: 10,\n    },\n    button2:{\n        backgroundColor: '#9400D3',\n        color: 'black',\n        width: '35%',\n        borderRadius:40,\n    },\n}));\n\n\nexport default function SignUp() {\n    const classes = useStyles();\n\n    const [user, setUser] = useState({\n        email: '',\n        password: '',\n        error: '',\n    });\n\n    const handleChange = e => {\n        setUser({\n            ...user,\n            [e.target.name]: e.target.value,\n            error: '',\n        })\n    };\n\n    const firebase = useFirebaseApp();\n\n    // Submit function (Log in user)\n    const handleSubmit = e => {\n        e.preventDefault();\n        // Log in code here.\n        firebase.auth().signInWithEmailAndPassword(user.email, user.password)\n            .then(result => {\n                if (!result.user.emailVerified) {\n                    setUser({\n                        ...user,\n                        error: 'Please verify your email before to continue',\n                    })\n                    firebase.auth().signOut();\n                }\n            })\n            .catch(error => {\n                // Update the error\n                setUser({\n                    ...user,\n                    error: error.message,\n                })\n            })\n    }\n\n    return (\n            <div className={classes.root}>\n                <Header/>\n                <Grid >\n                    <Grid item xs={5} className={classes.grid}>\n                        <Paper className={classes.paper}>\n                            <form onSubmit={handleSubmit}>\n                            <h1>Welcome back!</h1>\n                              <p>Please sign in below to continue</p>\n                                 <p className={classes.p1}>Email address</p>\n                                    <input\n                                        type=\"text\"\n                                        name=\"Email\"\n                                        className={classes.input1}\n                                        onChange={handleChange}\n                                    /><br></br>\n                                    <p className={classes.p1}>Password</p>\n                                    <input\n                                        type=\"password\"\n                                        name=\"password\"\n                                        className={classes.input}\n                                        onChange={handleChange}\n                                    /><br></br>\n                                        <Button classes={{root: classes.button1,}} type=\"submit\">\n                                            Sign in\n                                        </Button>\n                                            <p>or</p>\n                                        <Button classes={{root: classes.button2,}}>\n                                            Sign in with Gmail\n                                        </Button><br></br>\n                            <p >Forgot Password?</p>\n                            </form>\n                            {user.error && <h4>{user.error}</h4>}\n                        </Paper>\n                    </Grid>\n                </Grid>\n            <Footer/>\n        </div>\n\n    );\n}","/Users/harukakato/WebstormProjects/StressBox/app/src/Firebase/firebaseConfig.js",[],"/Users/harukakato/WebstormProjects/StressBox/app/src/components/SignOut.js",[],"/Users/harukakato/WebstormProjects/StressBox/app/src/Fire.js",[],{"ruleId":"76","severity":1,"message":"77","line":1,"column":17,"nodeType":"78","messageId":"79","endLine":1,"endColumn":25},{"ruleId":"76","severity":1,"message":"80","line":2,"column":27,"nodeType":"78","messageId":"79","endL