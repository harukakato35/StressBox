ton from '@material-ui/core/Button';\nimport { useFirebaseApp } from 'reactfire';\n\nconst useStyles = makeStyles((theme) => ({\n    root: {\n        flexGrow: 1,\n    },\n    box:{\n        position: 'relative',\n    },\n    button:{\n        position: 'absolute',\n        top: '50%',\n        left: '65%',\n        height: 60,\n    },\n    p:{\n        position: 'absolute',\n        top: '10%',\n        left: '65%',\n        height: 60,\n        fontSize: 80,\n    }\n}));\n\nexport default function Top() {\n    const classes = useStyles();\n\n    const firebase = useFirebaseApp();\n    console.log(firebase);\n\n    return (\n        <React.Fragment>\n            <Header/>\n                <Box\n                    className={classes.box}\n                    style={{\n                        backgroundImage: `url(${candles})`,\n                        backgroundSize: \"cover\",\n                        height: \"100vh\",\n                        color: \"#f5f5f5\"\n                }}>\n\n                    <Button variant=\"contained\" color=\"primary\" disableElevation className={classes.button}>\n                        Disable elevation\n                    </Button>\n                    <p className={classes.p}>Picked Just <br></br>For You</p>\n                </Box>\n            <Footer/>\n        </React.Fragment>\n    );\n}\n\n//console.log(firebase); making sure firebase is working","/Users/harukakato/WebstormProjects/StressBox/app/src/components/BasicComponents/Header.js",["66"],"/Users/harukakato/WebstormProjects/StressBox/app/src/components/BasicComponents/Footer.js",["67","68","69"],"import React from 'react';\nimport AppBar from '@material-ui/core/AppBar';\nimport Toolbar from '@material-ui/core/Toolbar';\nimport IconButton from '@material-ui/core/IconButton';\nimport Typography from '@material-ui/core/Typography';\nimport { makeStyles } from '@material-ui/core/styles';\nimport Paper from '@material-ui/core/Paper';\nimport Grid from '@material-ui/core